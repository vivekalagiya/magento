<?xml version="1.0" ?>
<layout>
    <adminhtml_browse_edit>
        <update handle="editor" />
        <reference name="content">
            <block type="browse/adminhtml_browse_edit" name="browse_edit" />
        </reference>
        <reference name="left">
            <block type="browse/adminhtml_browse_edit_tabs" name="browse_tab" />
        </reference>
        <reference name="js">
            <block type="adminhtml/catalog_product_edit_js" template="catalog/product/js.phtml" name="catalog_product_js" />
            <block type="core/template" template="catalog/wysiwyg/js.phtml" />
        </reference>
    </adminhtml_browse_edit>
    <adminhtml_browse_attribute_edit>
        <reference name="left">
            <block type="browse/adminhtml_browse_attribute_edit_tabs" name="browse_edit_tabs"></block>
        </reference>
        <reference name="content">
            <block type="browse/adminhtml_browse_attribute_edit" name="browse_edit_content"></block>
        </reference>
        <reference name="js">
            <block type="adminhtml/template" name="attribute_edit_js" template="browse/attribute/js.phtml"></block>
        </reference>
    </adminhtml_browse_attribute_edit>
</layout>